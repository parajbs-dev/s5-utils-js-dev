{
  "name": "s5-utils-js-dev",
  "version": "0.1.0-beta8-3-2",
  "description": "S5 Utils Javascript",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "not OperaMini all"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json && tsc --project tsconfig.build.cjs.json",
    "build-deploy": "rimraf bundle && mkdir bundle && browserify ./src/index.ts -s skynet -p [ tsify --noImplicitAny ] > bundle/index.js",
    "deploy": "yarn build-deploy && node ./scripts/deploy.js && rimraf bundle",
    "lint": "yarn lint:tsc && yarn lint:eslint",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:tsc": "tsc",
    "prettier": "prettier --write src/*.ts && prettier --write src/utils/*.ts",
    "prepublishOnly": "yarn && yarn build",
    "test": "echo test",
    "webpack": "npx webpack --config ./webpack.config.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parajbs-dev/s5-utils-js.git"
  },
  "keywords": [
    "Decentralised",
    "Content-Addressed",
    "Cloud Storage",
    "S5-Utils",
    "S5"
  ],
  "author": "parajbs-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parajbs-dev/s5-utils-js/issues"
  },
  "homepage": "https://github.com/parajbs-dev/s5-utils-js",
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@noble/hashes": "^1.3.1",
    "axios": "^1.4.0",
    "base32-encode": "^2.0.0",
    "buffer": "^6.0.3",
    "libsodium-wrappers": "^0.7.11",
    "mime": "^3.0.0",
    "path-browserify": "1.0.1",
    "url-join": "4.0.1",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/base64-js": "^1.3.0",
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/mime": "^3.0.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.2.5",
    "@types/randombytes": "^2.0.0",
    "@types/sjcl": "^1.0.30",
    "@types/url-join": "^4.0.1",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-loader": "^9.1.2",
    "eslint": "^8.43.0",
    "eslint-plugin-jsdoc": "46.2.6",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsify": "^5.0.4",
    "typescript": "5.0.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
